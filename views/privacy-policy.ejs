    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const toggleIcons = document.querySelectorAll(".toggle_icon");
            const sideMenu = document.querySelector(".side_menu");
            const closeMenuIcon = document.querySelector(".menu-close .toggle_icon");
    
            // Toggle the side menu when the toggle icon is clicked
            toggleIcons.forEach(icon => {
                icon.addEventListener("click", function () {
                    sideMenu.classList.toggle("active");
                });
            });
    
            // Close the side menu when the close menu icon is clicked
            closeMenuIcon.addEventListener("click", function () {
                sideMenu.classList.remove("active");
            });
    
            // Close the side menu if you click anywhere outside of it
            document.addEventListener("click", function (event) {
                if (!sideMenu.contains(event.target) && !event.target.closest('.toggle_icon')) {
                    sideMenu.classList.remove("active");
                }
            });
        });
    </script>
    
