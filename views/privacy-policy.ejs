/* Side menu styles */
.side_menu {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 350px;
    height: 100vh;
    background: linear-gradient(to right, #37d7e5, #00b8b8); 
    box-shadow: 2px 0 5px rgba(0, 0, 0, 0.3);
    z-index: 1000;
}

.side_menu.active {
    display: block;
}

/* Toggle Icon */
.toggle_icon {
    display: flex;
    flex-direction: column;
    cursor: pointer;
    position: fixed;
    top: 15px;
    left: 15px;
    z-index: 1100;
}

.toggle_icon span {
    background-color: #110e0e;
    height: 3px;
    width: 30px;
    margin: 4px 0;
}

/* Logo */
.logo img {
    max-width: 90%;
    object-fit: contain;
    height: 150px;
}

/* Interactive Border */
.interactive-border:hover {
    transform: scale(1.02);
    box-shadow: 0 8px 15px rgba(0, 0, 0, 0.2);
    border-color: #00b8b8;
}

/* ------- MOBILE RESPONSIVENESS -------- */

@media screen and (max-width: 768px) {
    .list.menu-left {
        text-align: left !important;
        padding-left: 0 !important;
    }

