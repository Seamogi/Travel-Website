        font-size: 18px !important;
        padding-left: 0 !important;
        text-align: left !important;
    }

    .side_menu {
        width: 250px !important; 
    }
}
 

    .toggle_icon {
        left: 10px;
        top: 10px;
    }

    .toggle_icon span {
        width: 25px; 
    }


