<script>
    document.querySelectorAll('.video-wrapper').forEach(wrapper => {
      const video = wrapper.querySelector('.service-video');
      const button = wrapper.querySelector('.play-button');
  
      // Show play button initially
      button.style.display = 'flex';
  
      // When play button is clicked
      button.addEventListener('click', () => {
        video.play();
        video.muted = false;
        button.style.display = 'none';
      });
  
      // Click anywhere on video to pause
      video.addEventListener('click', () => {
        if (!video.paused) {
          video.pause();
          button.style.display = 'flex';
        }
      });
  
      // Reset button when video ends
      video.addEventListener('ended', () => {
        button.style.display = 'flex';
      });
    });
  </script>
  
  
  
  
  
	

    <script>
    document.addEventListener("DOMContentLoaded", function () {
        const toggleIcons = document.querySelectorAll(".toggle_icon");
        const sideMenu = document.querySelector(".side_menu");
        const closeMenuIcon = document.querySelector(".menu-close .toggle_icon");

      
        toggleIcons.forEach(icon => {
            icon.addEventListener("click", function () {
                sideMenu.classList.toggle("active");
            });
        });


        closeMenuIcon.addEventListener("click", function () {
            sideMenu.classList.remove("active");
        });

    
        document.addEventListener("click", function (event) {
            if (!sideMenu.contains(event.target) && !event.target.closest('.toggle_icon')) {
                sideMenu.classList.remove("active");
            }
        });
    });
</script>

